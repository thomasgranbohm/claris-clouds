{
  "kind": "collectionType",
  "collectionName": "artworks",
  "info": {
    "singularName": "artwork",
    "pluralName": "artworks",
    "displayName": "Artwork",
    "description": ""
  },
  "options": {
    "draftAndPublish": true,
    "populateCreatorFields": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "external_link": {
      "type": "string",
      "regex": "^https:\\/\\/.+$",
      "required": true
    },
    "original_sold": {
      "type": "boolean",
      "default": false
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "name"
    },
    "year_of_creation": {
      "type": "integer"
    },
    "width": {
      "type": "decimal"
    },
    "height": {
      "type": "decimal"
    },
    "medium": {
      "type": "string",
      "required": false
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "sections.call-to-action",
        "sections.rich-text",
        "sections.showcase"
      ]
    }
  }
}
