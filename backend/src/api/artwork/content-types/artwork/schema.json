{
  "kind": "collectionType",
  "collectionName": "artworks",
  "info": {
    "singularName": "artwork",
    "pluralName": "artworks",
    "displayName": "Artwork",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "redbubble_link": {
      "type": "string",
      "regex": "^https:\\/\\/www\\.redbubble\\.com\\/.+"
    },
    "original_sold": {
      "type": "boolean",
      "default": false
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    }
  }
}
